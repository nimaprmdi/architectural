{
  "name": "architectural",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run build:sass\" \"next dev\"",
    "build": "concurrently \"npm run build:sass-build\" \"next build\"",
    "start": "next start",
    "lint": "next lint",
    "build:sass": "sass --watch --style compressed ./styles/scss/styles.scss ./styles/css/styles.css",
    "build:sass-build": "sass --style compressed ./styles/scss/styles.scss ./styles/css/styles.css"
  },
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@next/bundle-analyzer": "^14.0.4",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "inuitcss": "^6.0.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "swiper": "^11.0.5"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "daisyui": "^4.6.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "sass": "^1.69.7",
    "tailwindcss": "^3.3.0"
  }
}
